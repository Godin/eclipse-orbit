package-version=${version;===;${Bundle-Version}}
jna-version=${version;===;5.6.0}

Export-Package: \
 !about.html,!about_files, \
 *;version="${package-version}"

Import-Package: \
 com.sun.jna;version="${range;[===,=+);${jna-version}}", \
 com.sun.jna.ptr;version="${range;[===,=+);${jna-version}}", \
 com.sun.jna.win32;version="${range;[===,=+);${jna-version}}", \
 com.sun.jna*;version="${range;[===,=+);${jna-version}}", \
 *;resolution:=optional
